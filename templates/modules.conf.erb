# Cobbler module configuration file
# managed by Puppet

[authentication]
module = <%= @auth_module %>
<% if @hash_algorithm -%>
hash_algorithm = <%= @hash_algorithm %>
<% end -%>

[authorization]
module = <%= @authorization_module %>

[dns]
module = <%= @dns_option %>

[dhcp]
module = <%= @dhcp_option %>

[tftpd]
module = <%= @tftpd_option %>
